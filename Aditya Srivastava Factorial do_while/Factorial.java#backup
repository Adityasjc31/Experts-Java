import java.util.*;
class Factorial
{
    public static void main(String args [])
    {
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter a Number");int n=sc.nextInt();
        int f=1;
        for(int x=1;x<=n;x++)
        {
            f*=x;
        }
        System.out.println("Factorial="+f);
    }
}